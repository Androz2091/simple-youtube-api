{
	"name": "simple-youtube-api",
	"version": "6.0.0-dev",
	"description": "A library that wraps the YouTube Data API",
	"license": "Apache-2.0",
	"author": {
		"name": "Tyler Richards",
		"email": "hello@tjrgg.xyz"
	},
	"funding": "https://github.com/tjrgg/simple-youtube-api?sponsor=1",
	"repository": "tjrgg/simple-youtube-api",
	"engines": {
		"node": ">=12.0.0",
		"npm": "use-yarn",
		"yarn": ">=1.22.0"
	},
	"main": "src/index.js",
	"scripts": {
		"build": "tsc",
		"lint": "xo",
		"lint:fix": "xo --fix",
		"postversion": "git push && git push --tags"
	},
	"dependencies": {
		"iso8601-duration": "^1.2.0",
		"node-fetch": "^2.6.0"
	},
	"devDependencies": {
		"@types/node-fetch": "^2.5.7",
		"commitlint": "^8.3.5",
		"commitlint-config-enderverse": "enderverse/commitlint-config-enderverse",
		"dotenv": "^8.2.0",
		"eslint": "^7.0.0",
		"eslint-config-enderverse": "enderverse/eslint-config-enderverse",
		"husky": "^4.2.5",
		"typescript": "^3.8.3",
		"xo": "^0.30.0"
	},
	"commitlint": {
		"extends": [
			"enderverse"
		]
	},
	"eslintConfig": {
		"extends": [
			"enderverse"
		]
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"lint-staged": {
		"src/**/*.ts": [
			"xo --fix"
		]
	},
	"xo": {
		"extends": [
			"enderverse"
		],
		"rules": {
			"default-param-last": "off",
			"jsdoc/require-example": "off",
			"jsdoc/require-file-overview": "off",
			"jsdoc/require-jsdoc": "off",
			"@typescript-eslint/prefer-readonly-parameter-types": "off"
		},
		"space": false
	}
}
